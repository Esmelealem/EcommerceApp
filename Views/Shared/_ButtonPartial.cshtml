@model int


<td style="width:150px">
    <div class="btn-group" role="group">
        <a asp-action="Edit" class="btn btn-primary" href="@Url.Action("Edit/"+Model)">
            <i class="fas fa-edit"></i>
        </a>
        <a asp-action="Details" class="btn btn-success" href="@Url.Action("Details/"+Model)">
            <i class="fas fa-list"></i>
        </a>
        <a asp-action="Delete" class="btn btn-danger" href="@Url.Action("Delete/"+Model)">
            <i class="fas fa-trash-alt"></i>

        </a>
    </div>
</td>